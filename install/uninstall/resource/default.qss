/* 公共 */
QWidget {
    font-size: 14px;
    color: rgba(255,255,255,0.85);
    outline: none;
    background: transparent;
}

QPushButton {
    background: transparent;
}

QCheckBox::indicator, QTableView::indicator {
    width: 16px;
    height: 16px;
}
QCheckBox::indicator {
    margin-right: 2px;
}
QCheckBox::indicator:unchecked, QTableView::indicator:unchecked{border-image: url(:/uncheck.png);}
QCheckBox::indicator:unchecked:hover, QTableView::indicator:unchecked:hover{border-image: url(:/uncheckHover.png);}
QCheckBox::indicator:unchecked:pressed, QTableView::indicator:unchecked:pressed{border-image: url(:/uncheckHover.png);}
QCheckBox::indicator:unchecked:disabled, QTableView::indicator:unchecked:disabled{border-image: url(:uncheckDisabled.png);}
QCheckBox::indicator:checked, QTableView::indicator:checked{border-image: url(:/check.png);}
QCheckBox::indicator:checked:hover, QTableView::indicator:checked:hover{border-image: url(:/check.png);}
QCheckBox::indicator:checked:pressed, QTableView::indicator:checked:pressed{border-image: url(:/check.png);}
QCheckBox::indicator:checked:disabled, QTableView::indicator:checked:disabled{border-image: url(:/checkDisabled.png);}

QCheckBox::unchecked{color:#CFD3DC;}
QCheckBox::unchecked:hover{color:#E5EAF3;}
QCheckBox::unchecked:pressed{color:#E5EAF3;}
QCheckBox::unchecked:disabled{color:#E5EAF3;}
QCheckBox::checked{color:#E5EAF3;}
QCheckBox::checked:hover{color:#E5EAF3;}
QCheckBox::checked:pressed{color:#E5EAF3;}
QCheckBox::checked:disabled{color:#CFD3DC;}

QRadioButton::unchecked{color:#CFD3DC;}
QRadioButton::unchecked:hover{color:#E5EAF3;}
QRadioButton::unchecked:pressed{color:#E5EAF3;}
QRadioButton::unchecked:disabled{color:#E5EAF3;}
QRadioButton::checked{color:#E5EAF3;}
QRadioButton::checked:hover{color:#E5EAF3;}
QRadioButton::checked:pressed{color:#E5EAF3;}
QRadioButton::checked:disabled{color:#CFD3DC;}

QRadioButton::indicator {
    width: 16px;
    height: 16px;
}
QRadioButton::indicator:unchecked{border-image: url(:/unradio.png);}
QRadioButton::indicator:unchecked:hover, QRadioButton::indicator:unchecked[tab="1"]{border-image: url(:/unradioHover.png);}
QRadioButton::indicator:unchecked:pressed{border-image: url(:/unradioHover.png);}
QRadioButton::indicator:unchecked:disabled{border-image: url(:unradioDisabled.png);}
QRadioButton::indicator:checked{border-image: url(:/radio.png);}
QRadioButton::indicator:checked:hover, QRadioButton::indicator:checked[tab="1"]{border-image: url(:/radio.png);}
QRadioButton::indicator:checked:pressed{border-image: url(:/radio.png);}
QRadioButton::indicator:checked:disabled{border-image: url(:/radioDisabled.png);}

QScrollBar:vertical {
    width: 4px;
    background: rgba(0,0,0,0%);
}
QScrollBar:handle:vertical {
    border-radius: 2px;
    background: #4C4D4F;
}
QScrollBar:handle:vertical:hover {
    background: #6B6D71;
}
QScrollBar:handle:vertical:pressed {
    background: #8D9095;
}
QScrollBar::add-line:vertical {
    background:rgba(0,0,0,0%);
}
QScrollBar::sub-line:vertical {
    background:rgba(0,0,0,0%);
}
QScrollBar::add-page:vertical {
    background:rgba(0,0,0,0%);
}
QScrollBar::sub-page:vertical {
    background:rgba(0,0,0,0%);
}

QScrollBar:horizontal {
    height: 4px;
    background: rgba(0,0,0,0%);
}
QScrollBar:handle:horizontal {
    border-radius: 2px;
    background: #4C4D4F;
}
QScrollBar:handle:horizontal:hover {
    background: #6B6D71;
}
QScrollBar:handle:horizontal:pressed {
    background: #8D9095;
}
QScrollBar::add-line:horizontal {
    background:rgba(0,0,0,0%);
}
QScrollBar::sub-line:horizontal {
    background:rgba(0,0,0,0%);
}
QScrollBar::add-page:horizontal {
    background:rgba(0,0,0,0%);
}
QScrollBar::sub-page:horizontal {
    background:rgba(0,0,0,0%);
}

