MsgBox #headWidget {
background: #33353A;
border-top-right-radius:8px;
border-top-left-radius:8px;
border-bottom: 1px solid #4C4D4F;
}
MsgBox #bottomWidget,MsgBox #bodyWidget{
background: #2C2D31;
}
MsgBox #bottomWidget{
border-bottom-right-radius:8px;
border-bottom-left-radius:8px;
}
MsgBox #title {
    color: #E5EAF3;
    font-weight: bold;
}
MsgBox #titleIcon {
border-image: url(:/titleIcon.png);
}

/* 卸载 */
