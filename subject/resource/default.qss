QHeaderView {
    background: rgba(255, 255, 255, 0.06);
/*    border-bottom: 1px solid rgba(238,238,238,0.1);*/
    qproperty-defaultAlignment: AlignLeft;
    /*margin: 16px 0px 12px 0px;*/     /*之前布局影响*/
    border-radius:8px;
}
/* 若不加属性过滤，会影响垂直header的总高度 */
QHeaderView[horizontal=true] {
    min-height: 48px;
    max-height: 48px;
    border-radius: 8px;
}
QHeaderView::section {
    color: #CFD3DC;
    font-size: 12px;
    background: transparent;
    border: none;
    border-left: 1px solid #A3A6AD;
/*    border-right: 1px solid #A3A6AD;*/
    margin: 10px 0px 8px 0px;
    text-align: left;
    padding-top:6px;
    padding-left:10px;
}
QHeaderView::section:last {
/*    border: none;*/
}
QHeaderView::section:first {
    border: none;
}
QTableWidget {
    font-size: 14px;
    alternate-background-color: rgba(238,238,238,0.04);
    border-bottom-right-radius: 8px;
    qproperty-defaultAlignment: AlignLeft;
    background: transparent;
}
QTableWidget QScrollBar:vertical {
    margin-bottom: 8px;
    width: 8px;
}
QTableWidget QScrollBar:handle:vertical {
    margin-right: 4px;
}
QTableWidget QScrollBar:horizontal {
    margin-right: 8px;
    height: 8px;
}
QTableWidget QScrollBar:handle:horizontal {
    margin-bottom: 4px;
}
QTableWidget::item {
    color: #CFD3DC;
}
QTableWidget::item:selected {
    background-color: rgba(19,125,211,0.1);
}

QListWidget {
    background: rgba(0,0,0,0.3);
    border-radius: 2px;
    padding: 8px;
    font-size: 14px;
}
QListWidget::item {
    height: 32px;
    border-radius: 2px;
    background: transparent;
    color: #CFD3DC;
}
QListWidget::item:hover {
    height: 32px;
    border-radius: 2px;
    background: rgba(255, 255, 255, 0.06);
    color: #E5EAF3;
}
QListWidget::item:selected {
    color: #33A3FF;
    background: rgba(51, 163, 255, 0.10);
}

QProgressBar {
    border-radius: 3px;
    background: rgba(255,255,255,0.1);
}
QProgressBar::chunk {
    border-radius: 3px;
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 #409EFF, stop:1 #00D8C8);
}

