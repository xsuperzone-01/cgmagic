/* 公共 */
QWidget {
    font-size: 14px;
    color: #E5EAF3;
    outline: none;
    background: transparent;
}

QPushButton {
    background: transparent;
}

QCheckBox::indicator, QTableView::indicator {
    width: 16px;
    height: 16px;
}
QCheckBox::indicator {
    margin-right: 2px;
}
QCheckBox::indicator:unchecked, QTableView::indicator:unchecked{border-image: url(:/uncheck.png);}
QCheckBox::indicator:unchecked:hover, QTableView::indicator:unchecked:hover{border-image: url(:/uncheckHover.png);color:#E5EAF3;}
QCheckBox::indicator:unchecked:pressed, QTableView::indicator:unchecked:pressed{border-image: url(:/uncheckHover.png);color:#E5EAF3;}
QCheckBox::indicator:unchecked:disabled, QTableView::indicator:unchecked:disabled{border-image: url(:uncheckDisabled.png);color:#E5EAF3;}
QCheckBox::indicator:checked, QTableView::indicator:checked{border-image: url(:/check.png);color:#E5EAF3;}
QCheckBox::indicator:checked:hover, QTableView::indicator:checked:hover{border-image: url(:/check.png);color:#E5EAF3;}
QCheckBox::indicator:checked:pressed, QTableView::indicator:checked:pressed{border-image: url(:/check.png);color:#E5EAF3;}
QCheckBox::indicator:checked:disabled, QTableView::indicator:checked:disabled{border-image: url(:/checkDisabled.png);color:#CFD3DC;}

QCheckBox::unchecked{color:#CFD3DC;}
QCheckBox::unchecked:hover{color:#E5EAF3;}
QCheckBox::unchecked:pressed{color:#E5EAF3;}
QCheckBox::unchecked:disabled{color:#6C6E72;}
QCheckBox::checked{color:#E5EAF3;}
QCheckBox::checked:hover{color:#E5EAF3;}
QCheckBox::checked:pressed{color:#E5EAF3;}
QCheckBox::checked:disabled{color:#6C6E72;}

QRadioButton::unchecked{color:#CFD3DC;}
QRadioButton::unchecked:hover{color:#E5EAF3;}
QRadioButton::unchecked:pressed{color:#E5EAF3;}
QRadioButton::unchecked:disabled{color:#6C6E72;}
QRadioButton::checked{color:#E5EAF3;}
QRadioButton::checked:hover{color:#E5EAF3;}
QRadioButton::checked:pressed{color:#E5EAF3;}
QRadioButton::checked:disabled{color:#6C6E72;}

QRadioButton::indicator {
    width: 16px;
    height: 16px;
}
QRadioButton::indicator:unchecked{border-image: url(:/unradio.png);color:#CFD3DC;}
QRadioButton::indicator:unchecked:hover, QRadioButton::indicator:unchecked[tab="1"]{border-image: url(:/unradioHover.png);color:#E5EAF3;}
QRadioButton::indicator:unchecked:pressed{border-image: url(:/unradioHover.png);color:#E5EAF3;}
QRadioButton::indicator:unchecked:disabled{border-image: url(:unradioDisabled.png);color:#E5EAF3;}
QRadioButton::indicator:checked{border-image: url(:/radio.png);color:#E5EAF3;}
QRadioButton::indicator:checked:hover, QRadioButton::indicator:checked[tab="1"]{border-image: url(:/radio.png);color:#E5EAF3;}
QRadioButton::indicator:checked:pressed{border-image: url(:/radio.png);color:#E5EAF3;}
QRadioButton::indicator:checked:disabled{border-image: url(:/radioDisabled.png);color:#CFD3DC;}

QCheckBox, QRadioButton {
    color: rgba(255,255,255,0.65);
}
QCheckBox:disabled, QRadioButton:disabled {
    color: rgba(255,255,255,0.3);
}

QScrollBar:vertical {
